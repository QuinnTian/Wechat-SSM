<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="tk.mybatis.web.mapper.TextMessageMapper">

    <select id="selectAll" resultType="tk.mybatis.web.model.TextMessage">
        SELECT * FROM text_message a INNER JOIN base_message b on a.id=b.id;
    </select>
</mapper>