<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="tk.mybatis.web.mapper.BaseMessageMapper">

    <select id="selectAll" resultType="tk.mybatis.web.model.BaseMessage">
        SELECT * FROM base_message;
    </select>
    <insert id="insert" useGeneratedKeys="true" keyProperty="id" >
        insert base_message(to_user_name,from_user_name,create_time,msg_type) value (#{toUserName},#{fromUserName},#{createTime},#{msgType})
    </insert>
</mapper>